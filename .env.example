# Copy to .env and fill values
OPENAI_API_KEY=sk-...
PINECONE_API_KEY=...
PINECONE_ENV=us-west1-gcp
PINECONE_INDEX_NAME=malaysia-rag
OPENAI_CHAT_MODEL=gpt-3.5-turbo
# Twilio (for WhatsApp integration)
TWILIO_ACCOUNT_SID=ACxxxx
TWILIO_AUTH_TOKEN=xxxx
TWILIO_WHATSAPP_NUMBER=whatsapp:+1415...
# Map each Twilio WhatsApp number to a business ID (JSON object)
TWILIO_NUMBER_TO_BUSINESS={"whatsapp:+1415...":"default","whatsapp:+60...":"myshop"}
# Optional
PORT=5000
ADMIN_API_KEY=changeme-local
BUSINESS_CONFIG_PATH=business_config.json
